{
  "name": "dx-dev-docs-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["docusaurus"],
  "sourceRoot": "apps/dx/dev-docs-app/src",
  "targets": {
    "build": {
      "executor": "@peerlab/docusaurus:browser",
      "options": {
        "outputPath": "dist/apps/dx/dev-docs-app"
      }
    },
    "serve": {
      "executor": "@peerlab/docusaurus:dev-server",
      "options": {
        "port": 4300
      }
    },
    "serve:production": {
      "executor": "@peerlab/docusaurus:serve",
      "options": {
        "port": 4300,
        "outputPath": "dist/apps/dx/dev-docs-app"
      }
    }
  },
  "tags": []
}
