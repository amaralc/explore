{
  "name": "core-platform-shell-iac",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/core/platform-shell-iac",
  "projectType": "application",
  "implicitDependencies": ["researchers-peers-svc-iac", "core-platform-shell-browser", "dx-dev-docs-browser"],
  "targets": {
    "terraform:init": {
      "command": "cd apps/core/platform-shell-iac && terraform init"
    },
    "terraform:validate": {
      "command": "cd apps/core/platform-shell-iac && terraform validate"
    },
    "terraform:plan": {
      "command": "cd apps/core/platform-shell-iac && terraform plan -out=tfplan -var-file=env.tfvars -lock=false"
    },
    "terraform:apply": {
      "command": "cd apps/core/platform-shell-iac && terraform apply -lock=false -auto-approve tfplan && terraform output"
    },
    "terraform:destroy": {
      "command": "cd apps/core/platform-shell-iac && terraform destroy -var-file=env.tfvars -lock=false -auto-approve"
    },
    "terraform:output": {
      "command": "cd apps/core/platform-shell-iac && terraform output"
    }
  },
  "tags": []
}
