  "scripts": {
    "prepare": "husky install",
    "pre-commit": "yarn test:unit",
    "git:discard:all": "git checkout -- .",
    "release:no-push": "lerna version --yes --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "release": "lerna version --yes --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "consumer-with-api:serve": "make consumer-with-api-serve",
    "test:unit": "yarn nx run-many --target=test --all=true",
    "env:setup": "cp .env.example .env",
    "prisma:generate:postgres": "prisma generate --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:db:pull": "prisma db pull --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:migrate:dev:create-only": "prisma migrate dev --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma --create-only",
    "prisma:migrate:deploy": "prisma migrate deploy --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:studio": "prisma studio --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "start:example:mongoose": "yarn env:setup && yarn prisma:generate:postgres && nx run-many --target=serve --all=true",
    "start:example:prisma": "yarn env:setup && yarn prisma:generate:postgres && yarn prisma:migrate:dev && nx run-many --target=serve --all=true",
    "build:researchers-peers-svc-rest-api": "yarn prisma:generate:postgres && yarn nx run researchers-peers-svc-rest-api:build",
    "build:service-consumer": "yarn prisma:generate:postgres && yarn nx run service-consumer:build",
    "researchers-peers-svc-rest-api:serve:production": "node dist/apps/researchers/peers/svc/rest-api/main.js",
    "researchers-peers-svc-rest-api:serve": "make researchers-peers-svc-rest-api-serve",
    "service-consumer:serve": "make service-consumer-serve",
    "start": "yarn nx run-many --target=serve --all=true",
    "start:back-end": "nx run-many --target=serve --projects=researchers-peers-svc-rest-api,service-consumer --parallel --maxParallel=2",
    "e2e:back-end": "nx e2e core-peers-svc-e2e",
    "e2e:front-end": "nx e2e service-mfe-e2e"
  },
  "private": true,
