  "scripts": {
    "prepare": "husky install",
    "git:discard:all": "git checkout -- .",
    "release": "lerna version --yes --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "release:no-push": "lerna version --yes --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "prerelease": "lerna version prerelease --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "prerelease:no-push": "lerna version prerelease --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "conventional-prerelease": "lerna version --conventional-prerelease --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "conventional-prerelease:no-push": "lerna version --conventional-prerelease --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "conventional-graduate": "lerna version --yes --conventional-graduate --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "conventional-graduate:no-push": "lerna version --yes --conventional-graduate --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "preminor": "lerna version preminor --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "preminor:no-push": "lerna version preminor --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "premajor": "lerna version premajor --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js",
    "premajor:no-push": "lerna version premajor --preid beta --yes --conventional-commits --changelog-preset ./changelog-preset.config.js --no-push",
    "consumer-with-api:serve": "make consumer-with-api-serve",
    "test:unit": "nx run-many --target=test --all=true",
    "env:setup": "cp .env.example .env",
    "prisma:generate:postgres": "prisma generate --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:db:pull": "prisma db pull --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:migrate:dev:create-only": "prisma migrate dev --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma --create-only",
    "prisma:migrate:deploy": "prisma migrate deploy --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "prisma:studio": "prisma studio --schema libs/researchers/peers/adapters/src/database/infra/prisma/postgresql.schema.prisma",
    "start:example:mongoose": "pnpm env:setup && pnpm prisma:generate:postgres && nx run-many --target=serve --all=true",
    "start:example:prisma": "pnpm env:setup && pnpm prisma:generate:postgres && pnpm prisma:migrate:dev && nx run-many --target=serve --all=true",
    "build:researchers-peers-svc-rest-api": "pnpm prisma:generate:postgres && pnpm nx run researchers-peers-svc-rest-api:build",
    "build:service-consumer": "pnpm prisma:generate:postgres && pnpm nx run service-consumer:build",
    "researchers-peers-svc-rest-api:serve:production": "node dist/apps/researchers/peers/svc-rest-api/main.js",
    "researchers-peers-svc-rest-api:serve": "make researchers-peers-svc-rest-api-serve",
    "service-consumer:serve": "make service-consumer-serve",
    "start": "pnpm nx run-many --target=serve --all=true",
    "start:back-end": "nx run-many --target=serve --projects=researchers-peers-svc-rest-api,service-consumer --parallel --maxParallel=2",
    "e2e:back-end": "nx e2e core-peers-svc-e2e",
    "e2e:front-end": "nx e2e service-mfe-e2e"
  },
